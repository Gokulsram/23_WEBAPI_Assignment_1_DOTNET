@model IEnumerable<_23_WEBAPI_Assignment_1_DOTNET.POMasterModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h4>POMaster Details</h4><hr />
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PONO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PODATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SUPLNAME)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PONO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PODATE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SUPLNAME)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.PONO }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.PONO })
            </td>
        </tr>
    }

</table>
